  "directories": {
    "lib": "dist"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@splidejs/react-splide": "^0.7.12",
    "classnames": "^2.5.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.11.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "typescript": "^5.2.2",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3"
  }
